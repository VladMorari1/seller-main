{
  "name": "seller-preorders",
  "vendor": "etikette",
  "version": "2.1.92",
  "title": "Pre-orders management",
  "description": "The preorder app will store and manage the orders that will be marked as pre-ordered",
  "builders": {
    "react": "3.x",
    "messages": "1.x",
    "docs": "0.x",
    "admin": "0.x",
    "node": "6.x"
  },
  "dependencies": {
    "vtex.styleguide": "9.x"
  },
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.myvtex.com",
        "path": "/api/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/api/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/{{account}}/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "/api/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "ec2-13-211-128-127.ap-southeast-2.compute.amazonaws.com",
        "path": "/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.stripe.com",
        "path": "/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "whola.myvtex.com",
        "path": "/*"
      }
    },
    {
      "name": "OMSViewer"
    }
  ],
  "settingsSchema": {
    "title": "Pre-orders",
    "type": "object",
    "properties": {
      "api__accessToken": {
        "title": "Access key",
        "description": "",
        "type": "string"
      },
      "api__appKey": {
        "title": "AppKey",
        "description": "",
        "type": "string"
      },
      "api__appToken": {
        "title": "AppToken",
        "description": "",
        "type": "string",
        "format": "password"
      },
      "stripe_account_id": {
        "title": "Stripe Account ID*",
        "description": "Please obtain this ID from the marketplace administrator or support team.",
        "type": "string",
        "format": "password"
      },
      "notifications__emailList": {
        "title": "Add recipients email addresses",
        "description": "",
        "type": "array",
        "uniqueItems": true,
        "items": {
          "title": "Email",
          "type": "string",
          "format": "email"
        }
      }
    }
  },
  "credentialType": "absolute",
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema",
  "billingOptions": {
    "termsURL": "https://compliance.vtex.com/gdpr/policies/vtex-privacy-policy",
    "support": {
      "email": "leon.thor@vtex.com.br",
      "url": "https://support.vtex.com/hc/requests"
    },
    "free": true,
    "type": "free",
    "availableCountries": [
      "*"
    ]
  }
}
